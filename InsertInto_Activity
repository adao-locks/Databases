show databases;
create database newDBnew;
use newDBnew;

create table meus_contatos (
	sobrenome varchar(30),
    primeiro_nome varchar(20),
    email varchar(50),
    aniversario date,
    profissao varchar(50),
    endereco varchar(50),
    estado_civil varchar(20),
    interesses varchar(100),
    procura varchar(100)
);

